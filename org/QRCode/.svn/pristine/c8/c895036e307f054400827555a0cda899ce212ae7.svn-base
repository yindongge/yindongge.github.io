    font-family: inherit;
    font-size: 17px;
    margin: 5px 0 0;
    color: #333;
}
.self_set_popin{
	    position: fixed;
    z-index: 10000;
    top: 50%;
    left: 50%;
    display: block;
    overflow: hidden;
    width: 270px;
    -webkit-transition-property: -webkit-transform,opacity;
    transition-property: transform,opacity;
    -webkit-transform: translate3d(-50%, -50%, 0) scale(1);
    transform: translate3d(-50%, -50%, 0) scale(1);
    text-align: center;
    opacity: 1;
    color: #000;
    border-radius: 13px;
    background: #fff;
}
