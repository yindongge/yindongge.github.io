.mui-popup-button {
    font-size: 15px;
    line-height: 41px;
    position: relative;
    display: block;
    overflow: hidden;
    box-sizing: border-box;
    width: 100%;
    height: 41px;
    padding: 0 5px;
    cursor: pointer;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #333;
    background: rgba(255, 255, 255, .95);
    -webkit-box-flex: 1;
}
