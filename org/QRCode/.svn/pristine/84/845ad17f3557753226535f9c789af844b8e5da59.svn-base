  padding-left: 90px;
  margin-bottom: 10px;
}
.modal.special{
  width: 100%;
  display: block;
  opacity: 1;
  height: 450px;
  overflow: auto;
  padding-bottom: 40px;
}
.modal .modal-dialog{
  width: 100%;
  margin: 0px;
  padding:0px;

}
.modal.special .modal-content{
  box-shadow: none;
  border-radius: 0px;
  border: 0px;
}
.modal.special .modal-footer{
  border: 0px;
  margin-bottom: 40px;
}
