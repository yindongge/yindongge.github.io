.modal_mask {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    background: #fff;
    opacity: 0.3;
}
.read-text {
    width: 100%;
    overflow: hidden;
}
.read-list {
    display: block;
    width: 100%;
    min-height:  36px;
    color: #666;
}
.read-item-left {
    width: 50%;
    float: left;
}
.read-item-right {
    width: 50%;
    float: right;
}
.img_list_read{

}
.img_list_read img{
     width: 100px;
    height: 100px;
    margin-bottom: 10px;
    margin-right: 5px;
}
.padding_title{
      position: absolute;
    left: 15px;
    width: 90px;
    text-align: right;
    right: 0px;
}
