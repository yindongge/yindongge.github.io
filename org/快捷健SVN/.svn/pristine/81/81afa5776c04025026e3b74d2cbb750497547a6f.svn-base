					</select>
				</div>
				<div class="form-group">
					<label for="">二维码类型：</label>
					<select class="form-control" >
						<option value="">全部</option>
						<option value="">已发布</option>
						<option value="">已作废</option>
						<option value=""></option>
					</select>
				</div>
				<div class="form-group">
					<label for="">库房类型：</label>
