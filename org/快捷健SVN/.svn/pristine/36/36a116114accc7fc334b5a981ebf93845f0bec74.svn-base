/*全局css*/
/*定位*/
.load-first{
	width: 100%;
	min-height: 100%;
	background: #0061cf;
	position: fixed;
	top: 0px;
	left: 0px;
	right: 0px;
	bottom: 0px;
}
.loadtop{
	height: 45%;
	position: relative;
}
.loadtop .loadimgbox{
	width: 50%;
	height: auto;
	display: block;
	position: absolute;
	bottom: 70px;
	left: 50%;
	margin-left: -25%;
}
.loadimgbox img{
	width: 100%;
}
.load-text{
	text-align: center;
	position: absolute;
	bottom: 0px;
	color: #f5a633;
	font-size: 22px;
	width: 100%;
	height: 40px;
}
.loadbottom{
	height: 30%;
	position: relative;
	margin-top: 15%;
	width: 100%;
}
.shakeloading{
	width: 100%;
	margin: 10px auto;
	text-align: center;
}
.shakeloading img{
	width: 26px;
	height: 38px;
	-webkit-animation: solo 1.5s infinite ease-in-out;
	-o-animation: solo 1.5s infinite ease-in-out;
	animation: solo 1.5s infinite ease-in-out;
	position:relative;
}
@keyframes solo{
	0%{
		top: 0px;
	}
	50%{
		top: 10px;
	}
	100%{
		top: 0px;
	}
}
.loadcircle{
	width: 100%;
	height: 30px;
	text-align: center;
	margin: 5px auto;
	line-height: 30px;
}
.loadcircle .bounce{
	width: 6px;
	height: 6px;
	border-radius: 100%;
	-webkit-border-radius: 100%;
	display: inline-block;
	background:#fff;
	-webkit-animation:bounce 1s infinite ease-in-out;
	animation:bounce 1s infinite ease-in-out;
}
.bounce-text{
	color: #fff;
	font-size: 15px;
}
.loadcircle .bounce.bounce1{
	animation-delay:-0.4s;
	-webkit-animation-delay:-0.4s;
}
.loadcircle .bounce.bounce2{
	animation-delay:-0.3s;
	-webkit-animation-delay:-0.3s;
}
.loadcircle .bounce.bounce3{
	animation-delay:-0.2s;
	-webkit-animation-delay:-0.2s;
}
.loadcircle .bounce.bounce4{
	animation-delay:-0.1s;
	-webkit-animation-delay:-0.1s;
}
@-webkit-keyframes bounce{
	0%,20%,100%{
		-webkit-transform:scale(0,0);
	}
	40%{
		-webkit-transform:scale(1);
	}
}
@keyframes bounce{
	0%,20%,100%{
		-webkit-transform:scale(0,0);
	}
	40%{
		-webkit-transform:scale(1);
	}
}
/*结束定位*/





