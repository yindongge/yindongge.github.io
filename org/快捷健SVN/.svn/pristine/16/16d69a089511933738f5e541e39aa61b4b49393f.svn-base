<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<style type="text/css">
.center-content {
	margin: 0px auto;
}

.center-content img.yure {
	width: 100%;
}

.center-content.hot {
	margin-top: 0px;
}

.hotbox {
	background: #ea2262;
}

.hot-title {
	width: 602px;
	margin: 0px auto;
	text-align: center;
	margin-top: 15px;
}

.hot-list {
	width: 290px;
	height: 396px;
	display: block;
	float: left;
	background: #fff;
	overflow: hidden;
	position: relative;
	margin-right: 10px;
	margin-bottom: 24px;
	box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
}

.hot-list:hover .hot-text {
	background: #9cf7ff;
}

.hot-a {

}

.hot-img {
	width: 250px;
	height: 250px;
	overflow: hidden;
	margin: 0px auto;
	text-align: center;
}

.hot-img img {
	width: 250px;
	height: 250px;
}

.hot-text {
	height: 146px;
	padding-top: 8px;
	position: relative;
}

.hot-text h4 {
	font-size: 18px;
	color: #000;
	height: 30px;
	line-height: 30px;
	padding-left: 15px;
	margin-top: 10px;
	text-overflow: ellipsis;
	word-wrap: normal;
	overflow: hidden;
	white-space: nowrap;
	font-weight: normal;
}

.hot-price {
	position: absolute;
	height: 102px;
	bottom: 0px;
	left: 0px;
	right: 0px;
}

.hot-icon {
	display: inline-block;
	width: 100px;
	height: 52px;
	background: url(../common/img/hotspan.png) no-repeat;
	position: absolute;
	right: 0px;
	bottom: 18px;
}

.hot-delete {
	display: block;
	color: #b1acac;
	font-size: 18px;
	text-decoration: line-through;
	left: 15px;
	width: 138px;
	bottom: 55px;
	position: absolute;
}

.hot-normal {
	display: block;
	left: 15px;
	bottom: 14px;
	color: #ea2262;
	font-size: 26px;
	width: 138px;
	position: absolute;
}

.hot-normal i {
	font-size: 26px;
	font-style: normal;
}

