<body>
<div class="actAll">
	<div id="geren">
		<img src="img/11.jpg" alt="" class="bigimg">
	</div>
	<div id="qiye">
		<img src="img/22.jpg" alt="" class="bigimg">
	</div>
</div>
<div class="floor-panel" style="display: block;">
	<a class="bottoma"></a>
	<a href="#geren" target="_parent" class="geren"></a>
	<a href="#qiye" target="_parent" class="qiye"></a>
</div>
<script type="text/javascript">
$(function(){
	$(window).scroll(function(){
		if($(window).scrollTop()>400){
			$(" .floor-panel").fadeIn();
		}
		else{
			$(".floor-panel").hide();
		}
	});

	$(".floor-panel .bottoma").click(function(){
		$('html,body').animate({'scrollTop':0},900);
	});
})
</script>
</body>

