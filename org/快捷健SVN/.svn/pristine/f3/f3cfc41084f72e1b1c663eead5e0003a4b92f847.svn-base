  <!--  -->
<script type="text/javascript">
function animateAddCart(imgtodrag,cart){
		//var cart = $('.foot3');//终点
		//var imgtodrag = $(this).parents('.price_box').siblings('.pic_box').find('img');//列表的第一个图片
		var imgclone = imgtodrag.clone().offset({
			top: imgtodrag.offset().top + 50,
			left: imgtodrag.offset().left + 50
		}).css({
			'opacity': '0.5',
			'position': 'absolute',
			'height': '100px',
			'width': '100px',
			'z-index': '100'
		}).appendTo($('body')).animate({
			'top': cart.offset().top + 10,
			'left': cart.offset().left + 10,
			'width': 75,
			'height': 75
		}, 1000, 'easeInOutExpo');
		setTimeout(function () {
			cart.effect('shake', { times: 2}, 300);//购物车抖动次数
		}, 1500);//购物车抖动
		imgclone.animate({
			'width': 0,
			'height': 0
		}, function () {
			$(this).detach();
		});
}
</script>
