/*所有的都是公用的*/
a, abbr, acronym, address, applet, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, del, details, dfn, div, dl, dt, em, embed, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, label, legend, li, mark, menu, nav, object, ol, output, p, pre, q, ruby, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video{
	margin: 0px;
	padding: 0px;
	border: 0px;
	/*font-size: 100%;*/
	vertical-align:baseline;
}
html,body{
	height: 100%;
	-webkit-user-select:none;
	width: 100%;
	user-select:none;
}
div.scrollbug{
	overflow: scroll;
	-webkit-overflow-scrolling:touch;
}
html{
	font-family: 'Microsoft Yahei', 微软雅黑;
	-webkit-text-size-adjust:none;
	position: relative;.
	text-size-adjust:none;
	-webkit-font-smoothing:antialiased;
}
*{
	margin:0px;
	padding: 0px;
	border:none;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}

body.hidden{
	overflow: hidden;
}
ul, ol, li{
	list-style: none;
}
a{
	text-decoration: none;
}

.hide{
	display: none;
}
.padding-top{
	padding-top:44px;
}
.center{
	text-align: center;
}
.box{
	width: 100%;
	min-width: 320px;
	max-width: 640px;
	margin: 0px auto;
	-webkit-overflow-scrolling:touch;
	font-family: "Microsoft YaHei",微软雅黑;
	color: #333;
	position: relative;
	min-height: 100%;
	background: #000;
}
.wrapper{
	position:relative;
	animation:movemoon 5s  linear;
	top: 0px;


}
.wrapper img{
	width: 100%;
	
}
@keyframes movemoon{
	from{
		top: 0px;
	}
	to{
		top:-500px;
	}
}

.mask{
	position: fixed;
	top: 0px;
	left: 0px;
	bottom: 0px;
	right: 0px;
	background: #333;
	opacity: 0.7;
}
.img-mask {
	  width: 260px;
  height: 310px;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -155px;
  margin-left: -130px;
  background: #FFF;
  padding-top: 7px;
  border-radius: 8px;
  -webkit-border-radius: 8px;
}
.img-mask .title{
	height: 36px;
  line-height: 36px;
  font-size: 17px;
  color: #333;
  padding-left: 10px;
  font-weight: 600;
  text-align: center;
}
.img-mask p{
	padding: 0px 10px;
  font-size: 13px;
  line-height: 16px;
}
.form{
	background: #000;
	overflow: hidden;
}
.form-list{
	position: relative;
	width: 300px;
	height: 431px;
	background: url(../img/button.png) no-repeat center;
	margin: 0px auto;
	background-size: 100% 100%;
}
.btn{
	width: 100%;
	margin: 20px auto;
	background:#000;

}
.btn a{
	display: block;
	width: 80px;
	height: 40px;
	text-align: center;
	line-height: 40px;
	margin: 0px auto;
	background: url(../img/button.png) no-repeat center;
	background-size: 100% 100%;
}
.foot{
	background: #000;
	padding: 20px 0px;
}
.footimg{
	width: 300px;
	height: 40px;
	background: url(../img/foot.png) no-repeat center;
	margin: 0px auto;
}
.form-list input{
	background: transparent;
	height: 28px;
	width: 196px;
	color: #fff;
	padding-left: 5px;
	outline: none;
}
