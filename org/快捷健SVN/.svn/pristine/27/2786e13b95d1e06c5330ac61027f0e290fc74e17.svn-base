  text-align: center;
  padding-top: 50px;
  vertical-align: middle;
}
.modal_mask{
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    background: #fff;
    opacity: 0.3;
}
.form_group_control .form-group{
  margin-bottom: 10px;
}
