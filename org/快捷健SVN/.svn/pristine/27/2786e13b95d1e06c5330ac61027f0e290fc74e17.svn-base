.box{
  background: #fff;
}
.left {
  width: 170px;
  height: 100%;
  background: #EFEFEF;
  position: fixed;
  left: 0px;
  bottom: 0px;
  border: 1px solid #ccc;
}
.left-nav-control li a{
  color: #666;
}
.left .glyphicon{
  top: 4px;
  float: right;
  display: none;
}
.left li.active .glyphicon{
  display: block;
}
.fluid-control{
  padding-left: 5px;
}
.head-tip .breadcrumb{
  border: 1px solid #eee;
}
.form-control.control-special {
  display: inline-block;
  width: 60px;
  margin-right: 8px;
}
.well-control {
  height: 40px;
  line-height: 40px;
  padding: 0px;
  border: 0px;
  font-size: 12px;

}
.well-control .badge {
  background: #D9534F;
  padding: 1px 5px;
  margin-right: 10px;
}
.well-control .bg-warning{
  padding-left: 10px;
}
.form-well{
  padding: 5px;
  padding-left: 10px;
  display: block;
  margin-top: 10px;
  margin-bottom: 5px;
}
.form-well .form-group{
 margin-right: 20px;
}
.red{
  color: #f00;
}
.blue{
  color: #ff8a00;
}

.text-left{
  text-align: left;
}
.small_nav.navbar{
  border-top: 0px;
  min-height: 30px;
  background: #5bc0de;
}
.small_nav.navbar .nav a{
  padding:5px 15px;
}
.navbar-default .navbar-nav>.active>a{
  background: #ff8a00;
  color: #fff;
}
.left .nav-pills > li.active > a, .left .nav-pills > li.active > a:hover, .left .nav-pills > li.active > a:focus {
    color: #666;
    background-color: #fff;
    border-radius: 0px;
}
.table-bordered > thead > tr > th,.table-bordered > tbody > tr > td.text_center{
    border: 1px solid #DDD;
    text-align: center;
    font-size: 13px;
    font-weight: 400;
}
.table-bordered > tbody > tr > th,
.table-bordered > tfoot > tr > th,
.table-bordered > thead > tr > td,
.table-bordered > tbody > tr > td,
.table-bordered > tfoot > tr > td {
  text-align: left;
}

.pageselect {
  height: 35px;
  line-height: 35px;
  display: block;
  width: 100%;
  background-color: #EFEFEF;
}
.pageselect .pagination {
  margin: 0px 20px;
  position: relative;
  top: 2px;
  margin-right: 0px;
}
.pageselect .navbar-form {
  margin: 0px;
}
.pageselect .form-control{
 height: 30px;
 width: 80px;
}
.modal.in .modal-dialog{
  z-index: 1080;
}
.modal{

}
.modal-footer{
  text-align: center;
}
.modal-body .form-horizontal .control-label{
  padding: 0px;
  padding-top: 7px;
  font-weight: 400;
  font-size: 12px;
}
.glyphicon-repeat{
  margin-left: 5px;
  cursor: pointer;
  top: 3px;
}
.label_control{
  font-size: 12px;
  font-weight: 400;
}
.p_gray{
  color: #ccc;
}
.nopadding_h5{
  color: #333;
  font-size: 16px;
  font-weight: 800;
  padding-left: 5px;
}
.apply_control input{
  border: 0px;
  box-shadow: none;
}
.apply_control input:focus{
  border: none;
  box-shadow: none;
}
.clearfix .form-group{
  float: left;
  width: 50%;
  margin-left: 0px;
  margin-right: 0px;
}
.modal-body .form-horizontal  .clearfix .control-label{
  width: 85px;
}
.img_list {
  float: left;
  text-align: center;
}
.img_list .img_item{
  display: block;
  text-align: center;
  width: 70px;
  height: 70px;
  margin-right: 8px;
}
.spangroup{
  width: 70px;
  height: 24px;

}
.spangroup span{
      width: 33%;
    height: 24px;
    line-height: 24px;
    padding: 0px;
    margin: 0px;
    display: inline-block;
    float: left;
    color: #666;
}
.col-sm-5.special{
