}
.text-decertion-a{
  text-decoration: underline;
  cursor: pointer;
}
.table-bordered > tbody > tr > td.padding20{padding-left: 20px;}
.table-bordered > tbody > tr > td.padding40{padding-left: 40px;}
.table-bordered > tbody > tr > td.padding60{padding-left: 60px}
.bottom_btn{
  width: 100%;
