/*所有的都是公用的*/
a, abbr, acronym, address, applet, article, aside, audio, b, big, blockquote, body, canvas, caption, center, cite, code, dd, del, details, dfn, div, dl, dt, em, embed, fieldset, figcaption, figure, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, label, legend, li, mark, menu, nav, object, ol, output, p, pre, q, ruby, s, samp, section, small, span, strike, strong, sub, summary, sup, table, tbody, td, tfoot, th, thead, time, tr, tt, u, ul, var, video{
	margin: 0px;
	padding: 0px;
	border: 0px;
	/*font-size: 100%;*/
	vertical-align:baseline;
}
i{
	font-style: normal;
	font-weight: 400;
}
html,body{
	height: 100%;
	-webkit-user-select:none;
	width: 100%;
	user-select:none;
}
div.scrollbug{
	overflow: scroll;
	-webkit-overflow-scrolling:touch;
}
html{
	font-family: 'Microsoft Yahei', 微软雅黑;
	-webkit-text-size-adjust:none;
	position: relative;.
	text-size-adjust:none;
	-webkit-font-smoothing:antialiased;
}
*{
	margin:0px;
	padding: 0px;
	border:none;
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}
.bggray{
	background: #efefef;
}
.samebtn.bggray{
	background: #ccc;
	color: #999;
}
body{
	font-size: 12px;
}
body.hidden{
	overflow: hidden;
}
.hidden .box{
	height: 100%;
	overflow: hidden;
}
ul, ol, li{
	list-style: none;
}
dl{
	margin: 0px;
	padding: 0px;
}
a{
	text-decoration: none;
}
.strong{
	font-weight: 600;
}
.space2{
	margin-left: 10px;
}
.red{
	color: #f00;
}
.blue{
	color: #0061CF;
}
.hide{
	display: none;
}
.padding-top{
	padding-top:44px;
}
.center{
	text-align: center;
}
.box{
	width: 100%;
	min-width: 320px;
	max-width: 640px;
	margin: 0px auto;
	-webkit-overflow-scrolling:touch;
	font-family: "黑体";
	color: #333;
	position: relative;
	min-height: 100%;
	z-index: 2;

}

.box-control{
	padding-bottom: 50px;
	background: #efefef;
}
.swiper-slide img{
	width: 100%;
}
.yellow{
	color: #ff8a00;
}
.pull-right{
	float: right;
}

.rmb{
	font-style: normal;
	font-family: "Microsoft YaHei","Glyphicons",微软雅黑;
}
.english{
	font-style: normal;
	font-family: Arial;
}
.d-z-left  .english {
  font-style: normal;
  font-family: Arial;
  font-size: 18px;
  font-weight: 600;
}
.d-z-left .red.line-trough .english {
  font-style: normal;
  font-family: Arial;
  font-size: 12px;
  font-weight: 400;
}
.icon-right{
	background: url(../images/go.png) no-repeat center;
	width: 19px;
	height: 28px;
	display: inline-block;
	background-size: 10px 14px;
	position: absolute;
	right: 6px;
	top: 6px;
}
.box-gray{
	background: #efefef;
}
.gray{
	color: #ccc;
}
.overHide{
	height: 100%;
	overflow: hidden;
}
/*head*/
.header{
	height: 44px;
	position: relative;
	width: 100%;
	clear: both;
	overflow: hidden;
	background:#0061cf;
	z-index: 200;

}
// .box.nofixed{
// 	padding-top: 0px;
// }
// .box.nofixed .header{
// 	position: relative;
// }
.header.reverse-position{
	position: absolute;
	top: 0px;

}
.header.fixedheader{
	position: fixed;
	z-index: 10001;
}
.readyfixed.fixedme{
	position: fixed;
	top: 44px;
	z-index: 10001;

}

.header.fixed{
	z-index: 200;
	position: fixed;
	top: 0px;
	left: 0px;
	right: 0px;
}
.head-content{
	position: absolute;
	line-height: 44px;
	font-size: 18px;
	color: #fff;
	text-align: center;
	bottom: 0px;
	top: 0px;
	left: 44px;
	right: 44px;
}
.head-content.classify{
	right: 10px;
	line-height: 0px;

}
.head-content.classify.newsearch {
	left: 10px;
	right: 10px;
}
.head-content.classify input{
	width: 100%;
	height: 30px;
	margin-top: 7px;
	-webkit-border-radius: 	5px;
	-moz-border-radius: 	5px;
	-ms-border-radius: 	5px;
	-o-border-radius: 	5px;
	border-radius: 	5px;
	padding:0px 25px 0px 5px;
	outline:none;
	border:0px;
	padding-top: 1px;
	font-size: 12px;
}
.classify .searchicon{
	top: 1px;
  background: url(../images/search.png) no-repeat center;
  background-size: 17px 20px;
  right: -7px;
}
.head-left{
	height: 44px;
	width: 44px;
	line-height: 44px;
	position: absolute;
	top: 0px;
	background:url(../images/back.png) no-repeat center;
	background-size: 11px;
}
.head-left a.link{
	width: 100%;
	height: 100%;
	position: absolute;top: 0px;left: 0px;
}
.head_delete{
	width: 44px;
	height: 44px;
	position: absolute;right: 5px;
	line-height: 44px;
	text-align: center;
	font-size: 14px;
	color: #fff;

}
input::-webkit-input-placeholder
 {
    color: #ccc;
    font-family: "黑体";

}
.btn-wrapper{
	position: relative;
	width: 100%;
	padding: 15px ;
}
.btn{
	height: 50px;
	width: 100%;
	line-height: 50px;
	text-align: center;
	color: #fff;
	background: #ff8a00;
	font-size: 18px;
	border-radius: 5px;
	-webkit-border-radius:5px;
	display: block;
}
.head-content.classify.searchAdd {
  padding-right: 60px;
}
.shaixuan {
  width: 50px;
  height: 30px;
  display: block;
  border: 1px solid #FFF;
  position: absolute;
  right: 0px;
  top: 8px;
  text-align: center;
  line-height: 30px;
  border-radius: 5px;
  -webkit-border-radius: 5px;
  color: #FFF;
  font-size: 14px;
}
/*end head*/


/*footer*/
.footer{
	width: 100%;
	margin: 0px auto;
	text-align: center;
	margin-top: 30px;
}
.footer p{
	margin-bottom: 20px;
	color: #666;
}
.footer p:first-child{
	font-size: 14px;
}
.footer p:last-child{
	font-size: 16px;
}
.footer img{
	width: 100%;
}
.footerbar{
	position: fixed;
	height: 50px;
	width: 100%;
	left: 0px;
	right: 0px;
	bottom:0px;
	border-top: 1px solid #ccc;
	background:#fff;
	z-index: 499;
}
.footerbar a{
	display: block;
	float: left;
	width: 25%;
	text-align: center;
	font-size: 12px;
	color: #666;
	height: 50px;
}
.iconfoot{
	width: 25px;
	height: 19px;
	display: block;
	margin: 0px auto;
	margin-top: 8px;
margin-bottom: 4px;
position: relative;
}
