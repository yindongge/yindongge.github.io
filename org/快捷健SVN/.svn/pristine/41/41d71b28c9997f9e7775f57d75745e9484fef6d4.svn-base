.on .iconfoot.foot5{
	background: url(../images/i5.png) no-repeat center;
	background-size: 19px 19px;
}
.on .iconfoottext{
	color:#0061cf;
}
/*end footer*/

/*遮罩*/
.mask{
	position: fixed;
	top: 0px;
	left: 0px;
	right: 0px;
	bottom: 0px;
	background: #000;
	opacity: 0.5;
	z-index: 505;
	display: none;
}
.cover{
	position: fixed;
	top: 0px;
	left: 0px;
	right: 0px;
	bottom: 0px;
	background: #000;
	opacity: 0.5;
	z-index: 495;
	display: none;
}
.loading .loadcircle{
	height: 50px;
	line-height: 50px;
	margin: 0px auto;
}
.loading-block{
	width: 170px;
	height: 50px;
	position: fixed;
	top:50%;
	left: 50%;
	margin-top: -25px;
	margin-left: -85px;
	background: #333;
	opacity: 0.8;
	text-align: center;
	line-height: 50px;
	color: #fff;
	font-size: 18px;
	border-radius: 8px;
	-webkit-border-radius:8px;
	z-index: 565;
}
.loading-block.map-block{
	top: 140px;
}
/*遮罩*/
/*返回顶部*/
.gotop {
  width: 44px;
  height: 44px;
  position: fixed;
  bottom: 18%;
  background: url(../images/icon_top.png) rgba(255, 138, 0, 0.5) no-repeat center;
  float: right;
  right: 10px;
  z-index: 15000;
  display: none;
  background-size: 25px;
  border-radius: 100%;
  -webkit-border-radius: 100%;
}
.fixed-shopcar {
  width: 44px;
  height: 44px;
  position: fixed;
  bottom: 27%;
  background: url(../images/float_car.png) no-repeat rgba(255, 138, 0, 0.7) center;
  float: right;
  right: 10px;
  z-index: 15000;
  background-size: 26px;
  border-radius: 100%;
  -webkit-border-radius: 100%;
}
.img-circle {
  width: 5px;
  height: 5px;
  background: #FF8A00;
  display: block;
  color: #FFF;
  position: absolute;
  border-radius: 100%;
  -webkit-border-radius: 100%;
  text-align: center;
  font-size: 13px;
  right: 4px;
  top: 5px;
}
.backhome{
	position: fixed;
	height: 55px;
	display: block;
	right: 0px;
	bottom: 70px;
	z-index: 609;
}
.touchback{
	height: 41px;
	width: 41px;
	background: url(../images/side.png) no-repeat center ;
	background-size:70%;

	position: absolute;
	right: 0px;
	top: 0px;
}
.touchlist{
	height: 40px;
	width: 170px;
	background: #333;
	position: relative;
	right: -170px;
	opacity: 0.65;
	border-bottom-left-radius:20px;
	border-top-left-radius:20px;

}
.touchlist a{
	height: 40px;
	display: inline-block;
	width: 40px;
	float: right;
	padding: 3px;
	box-sizing:border-box;
}
.sideleft .touchlist{
	right: 0px;
	transition:all 0.5s ease-in-out;
	-webkit-transition:all 0.5s ease-in-out;
}
.sideleft .touchback{
	display: none;
}
.carone{
	background: url(../images/i8.png) no-repeat center;
	background-size: 14px 16px;
}
.sideone{
