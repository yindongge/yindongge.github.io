// JavaScript Document
$(function(){
	$('.tab1 .tdfirst').click(function(){
	$(this).parent('.tab1').siblings('.tab2').toggle();
	})

	})
$(function(){


		$('.taball .tdfirst').click(function(){
			$(this).parent('.taball').siblings('.tab1').toggle();
				$(this).parent('.taball').siblings('.tab2').hide();

			})
	})
function AutoAlert(str){
	$('body').append("<div class='fixed'>"+str+"</div>");
	$(".fixed").fadeIn();
	setTimeout(function(){
			$('.fixed').remove();
	},1500)
};