.sign_up_time{
	    width: 100%;
    min-height: 30px;
    font-size: 12px;
    color: #999;
    margin: 20px 0px;
    margin-bottom: 80px;
	.time{
		width: 28px;
		height: 34px;
		display: inline-block;
		border: 1px solid #c8c8c8;
		font-size: 18px;
		color: #c80000;
		font-family: Arial;
		text-align: center;line-height: 34px;
		font-weight: bold;
	}
	.maohao{
		width: 3px;
		height: 11px;
		display: inline-block;
		background: url(../img/icon/sg_10.png) no-repeat center;
		margin: 0px 4px;
	}
	&.no{
		.time{
			color: #999;
		}
	}
}
//
.sign_up_detail{
	width: 100%;
	background: #fff;
	min-height: 50px;
	h5{
		width: 100%;
		height: 40px;
		line-height: 40px;
		border-bottom: 1px solid #f5f5f5;
		position: relative;
		span{
			display: inline-block;
			min-width: 80px;
			border-bottom: 1px solid #ffaa00;
			margin-top: -1px;
			text-align: center;
		}
		a{
			width: 230px;
			height: 40px;
			float: right;
			text-align: center;line-height: 40px;
			color: #fff;
			background: #ffaa00;
		}
	}
	.sign_up_detail_content{
		width: 100%;
		padding: 10px;

	img{
		width: 100%;
		margin-bottom: 10px;
		}
	}
}
.sign_up_detail.fix h5{
	position: fixed;
	top: 0px;
	width: 1190px;
	background: #fff;
}