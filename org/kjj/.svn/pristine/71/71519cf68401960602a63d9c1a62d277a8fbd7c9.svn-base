@charset "utf-8";
.w1190{
	width: 1190px;
	margin: 0px auto;
}
.yc{
	color: #ffaa00;
}
.logo.logo_new{
	.logo_set_title{
		position: absolute;
	    font-size: 18px;
	    color: #666;
	    top: 40px;
	    left: 118px;
	}
}
.header.reverse_header{
	height: 112px;
	padding-top: 0px;
	display: block;
	.logo{
		height: 110px;
		padding-top: 19px;
	}
}
.new_set_nav{
	float: left;
	height: 112px;
	min-width: 500px;
	.nav_list{
		width: 152px;
		height: 100%;
		float: left;
		a{
			height: 72px;
		    width: 152px;
		    display: block;
		    text-align: center;
		    color: #333;
		    font-size: 16px;
		    padding-top: 46px;
		 &:hover{
		 	background: #ffaa00;
		 	color: #fff;
		 }
		 &:hover .toggle_nav_icon{
		 	width: 12px;
		height: 8px;
		background: url(../img/icon/sg_03.png) no-repeat center;
		display: inline-block;
		top: -4px;
    	left: 4px;
    	position: relative;
		 }
		 .bottom_nav_span{
		 	    height: 20px;
		    position: relative;
		    bottom: 5px;

		 }
		 &:hover .toggle_set_bottom{
		 	display: block;
		 }
		}
	.toggle_nav_icon{
		width: 12px;
		height: 8px;
		background: url(../img/icon/sg_05.png) no-repeat center;
		display: inline-block;
		top: -4px;
    	left: 4px;
    	position: relative;
	}
	.toggle_set_bottom{
		background: #fff;
		display: none;
		p{
			height: 44px;
			line-height: 44px;
			text-align: center;
			border-bottom: 1px solid #e6e6e6;
			font-size: 14px;
			color: #666;
			margin-bottom: 0px;
			&:hover{
				color: #ffaa00;
			}
		}
	}
	&.on{
		background: #ffaa00;
	}
	&.special{
		line-height: 106px;
		text-align: center;
		font-size: 16px;
		color: #333;
	}
	}
}
.sign_up_title{
	.w1190;
	h5{
		width: 100%;
		height: 54px;
		line-height: 54px;
		padding-left: 0px;
	}
	.set_yellow{
		font-size: 14px;
		color: #ffaa00;
	}
}
