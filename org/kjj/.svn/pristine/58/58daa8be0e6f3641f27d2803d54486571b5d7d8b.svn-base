<script type="text/javascript">
$(function(){
	$(window).scroll(function(){
		var scrollH=$(window).scrollTop();
	    var windowH=$(window).height();
		var loveH=$(".well").offset().top;
		if(scrollH+windowH>loveH)
		{
			$('.car-foot-box').removeClass('fix');
		}
		else
		{
			$('.car-foot-box').addClass('fix');
		}
	});
	$('.fixed_content ').click(function(){
		$(this).toggleClass('on')
		.find('.shopcar_fixed_span')
		.toggleClass('hover')
		.find('.fixed_div');
		$(this).parents('.list')
		.siblings('.list')
		.find('.fixed_content')
		.removeClass('on')
		.find('.shopcar_fixed_span')
		.removeClass('hover');
	});
});
</script>
