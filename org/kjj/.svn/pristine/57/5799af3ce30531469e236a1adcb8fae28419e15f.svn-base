<script type="text/javascript">
$(function(){
	$('.hover_new_car').on('click',function(e){
	animateAddCart($('.side_item.side_2'),$(this).parents('#findme').find('img'));
	e.preventDefault();
});
});

function animateAddCart(cartEnd,imgtodrag){
		var imgclone = imgtodrag.clone().offset({
			top: imgtodrag.offset().top + 50,
			left: imgtodrag.offset().left + 50
		}).css({
			'opacity': '0.5',
			'position': 'absolute',
			'height': '100px',
			'width': '100px',
			'z-index': '100'
		}).appendTo($('body')).animate({
			'top': cartEnd.offset().top + 0,
			'left': cartEnd.offset().left + 0,
			'width': 40,
			'height': 40
		}, 1000, 'easeInOutExpo');
		setTimeout(function () {
			cartEnd.effect('shake', { times: 2}, 300);//购物车抖动次数
		}, 1500);//购物车抖动
		imgclone.animate({
			'width': 0,
			'height': 0
		}, function () {
			$(this).detach();
		});
}
</script>
