<script type="text/javascript">
$(function(){
	// $(window).scroll(function(){
	// if($(window).scrollTop()>800){
	// 	$(" .go-top").fadeIn();
	// }
	// else{
	// 	$(".go-top").hide();
	// }
	// });
	// $(".go-top").click(function(){
	// 	$('html,body').animate({'scrollTop':0},900);
	// });
	$(window).scroll(function() {
		var indexStop=$(document).scrollTop();
		var slideBar=$('.side_left_toolbar');
		var future=$('.all').find('.future');
		var shoeFuture=$('#floor1').offset().top;
		var curID='';
		if(indexStop>shoeFuture-200)
		{
			slideBar.show();
		}
		else{
			slideBar.hide();
		}
		future.each(function(){
			var _this=$(this);
			var futureTop=_this.offset().top;
			if(indexStop>futureTop-50)
			{
				curID="#"+_this.attr('id');
			}
			else{
				return false;
			}
			var curLink=slideBar.find('.on');
			if(curID&&curLink.attr("href") !=curID)
			{
				curLink.removeClass('on');
				slideBar.find('[href='+ curID +']').addClass('on');
			}
		});

	});
	$('.select_top').click(function(){
		$(this).toggleClass('on').next('.select_content').toggleClass('active');
	});
	$('.select_input').focus(function(){
		$(this).parents('.select_second ').find('.select_content').addClass('active');
	});
	$('.pic').hover(function(){
		$(this).addClass('on');
		$(this).find('.block_bottom_join').stop().animate({
			"bottom":"0px",
		},"fast");
	},function(){
		$(this).removeClass('on');
		$(this).find('.block_bottom_join').stop().animate({
			"bottom":"-60px",
		});
	});
	$(".thumbnail").hover(function(){
		$(this).find('.hover_new_car').show();
	},function(){
		$(this).find('.hover_new_car').hide();
	});
});

</script>
