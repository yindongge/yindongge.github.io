package com.kjj.commserver.entity.goods.aide;

import com.kjj.commserver.entity.goods.OrgBrand;

public class OrgBrandQuery extends OrgBrand {
	
	/** 商品分类id */
	private Integer classId;
	
	/**分类等级id*/
	private Byte classLevel;
	
	/**关联分类id字符串*/
	private String classIds;
	
	/** 商品分类父id */
	private Integer classParentId;

	public Integer getClassId() {
		return classId;
	}

	public void setClassId(Integer classId) {
		this.classId = classId;
	}

	public Byte getClassLevel() {
		return classLevel;
	}

	public void setClassLevel(Byte classLevel) {
		this.classLevel = classLevel;
	}

	public String getClassIds() {
		return classIds;
	}

	public void setClassIds(String classIds) {
		this.classIds = classIds;
	}

	public Integer getClassParentId() {
		return classParentId;
	}

	public void setClassParentId(Integer classParentId) {
		this.classParentId = classParentId;
	}




	
	
}