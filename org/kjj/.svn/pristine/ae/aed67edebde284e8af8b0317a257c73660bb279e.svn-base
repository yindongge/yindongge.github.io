.side_item{
	width: 40px;
	height: 40px;
	background: #2f2a20;
	margin-bottom: 10px;
	position: relative;
	cursor: pointer;
}
.side_item:after,.side_item:before,.side_item{
		-webkit-transition: all 0.25s ease-in 0s;
    -moz-transition: all 0.25s ease-in 0s;
    -ms-transition: all 0.25s ease-in 0s;
    -o-transition: all 0.25s ease-in 0s;
    transition: all 0.25s linear 0s;
}
.side_item:hover::before{
	width: 88px;
}
.side_item:hover::after{
	display: block;
}
.side_item:after{
    position: absolute;
    right: 34px;
    content: "";
    width: 0px;
    height: 0px;
    border-top: 6px solid transparent;
    border-bottom: 6px solid transparent;
    border-left: 6px solid #666;
    top: 17px;
    display: none;
}
.side_item:before{
	position: absolute;
	right: 40px;
	top: 0px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #fff;
	background: #7f7052;
	content: "";
	overflow: hidden;
	width: 0px;
}
.side_item.side_1:before{
	content: "个人中心";
}
.side_item.side_2:before{
	content: "购物车";
}
.side_item.side_3:before{
	content: "优惠券";
}
.side_item.side_4:before{
	content: "我的订单";
}
.side_item.side_5:before{
	content: "返回顶部";
}
