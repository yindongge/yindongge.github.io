.center-content {
	margin: 0px auto;
}

.center-content img.yure {
	width: 100%;
}

.center-content.hot {
	margin-top: 0px;
}

.hotbox {
	background: #ea2262;
}

.hot-title {
	width: 602px;
	margin: 0px auto;
	text-align: center;
	margin-top: 15px;
}
.hot-list {
	    width: 100%;
    height: 360px;
    display: block;
    float: left;
    background: #fff;
    overflow: hidden;
    position: relative;
    margin-right: 10px;
    margin-bottom: 24px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.5);
}
.hot-img {
	width: 240px;
	height: 240px;
	overflow: hidden;
	margin: 0px auto;
	text-align: center;
	padding-top: 10px;
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.hot-img img {
	width: 220px;
	height: 220px;
}

.hot-text {
	height: 120px;
	position: relative;
	overflow: hidden;
}

.hot-text h4 {
	font-size: 14px;
	color: #333;
	line-height: 18px;
	padding-left: 15px;
	height: 36px;
	font-weight: normal;
	overflow: hidden;
}

.hot-price {
	position: absolute;
	height: 84px;
	bottom: 0px;
	left: 0px;
	right: 0px;
}

.hot-icon {
	display: inline-block;
	width: 50px;
	height: 50px;
	background: url(../img/car1.png) no-repeat;
	position: absolute;
	right: 10px;
	bottom: 18px;
}
.hot-icon.on{
	background: url(../img/car2.png) no-repeat;
}
.hot-delete {
	display: block;
	color: #999;
	font-size: 12px;
	text-decoration: line-through;
	left: 15px;
	width: 138px;
	bottom: 55px;
	position: absolute;
}

.hot-normal {
	  display: block;
    left: 15px;
    bottom: 24px;
    color: #e72864;
    font-size: 18px;
    width: 138px;
    position: absolute;
}

.hot-normal i {
	font-style: normal;
}

