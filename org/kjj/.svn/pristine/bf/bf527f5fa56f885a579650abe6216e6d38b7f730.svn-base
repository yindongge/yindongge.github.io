package com.kjj.commserver.entity.goods.aide;

import java.util.Collection;

import com.kjj.commserver.entity.goods.OrgProductPropertyValue;

public class OrgProductPropertyValueQuery extends OrgProductPropertyValue {
	/**属性id集合*/
	private Collection<Integer> propertyIds;
	
	/** 对应 org_class.class_Id */
	private Integer classId;

	public Collection<Integer> getPropertyIds() {
		return propertyIds;
	}

	public void setPropertyIds(Collection<Integer> propertyIds) {
		this.propertyIds = propertyIds;
	}

	public Integer getClassId() {
		return classId;
	}

	public void setClassId(Integer classId) {
		this.classId = classId;
	}
	
}