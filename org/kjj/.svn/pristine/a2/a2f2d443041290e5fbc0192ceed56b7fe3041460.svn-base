/*图片小图预览列表*/
.spec-preview{width:350px;height:350px;border:1px solid #DFDFDF; overflow: hidden;}
.spec-scroll{clear:both;margin-top:16px;width:352px;overflow: hidden;}
.spec-scroll .prev{float:left;}
.spec-scroll .next{float:right;}
.spec-scroll .prev,.spec-scroll .next{
	display:block;
	text-align:center;width:24px;height:54px;
	line-height:54px;
	cursor:pointer;text-decoration:none;

}
.spec-scroll .glyphicon {
    font-size: 25px;
    color: #ccc;
    top: 10px;
}
.spec-scroll .glyphicon:hover{
	color: #999;
}
.spec-scroll .glyphicon-menu-left{

}
.spec-scroll .glyphicon-menu-right{

}
.spec-scroll .items{float:left;position:relative;width:304px;height:56px;overflow:hidden;}
.spec-scroll .items ul{position:absolute;width:999999px;height:56px; padding: 0px;}
.spec-scroll .items ul li{float:left;width:64px;text-align:center; height: 50px; width: 60px; /*margin:0px 5px;*/
}
.spec-scroll .items ul li:first-child{
	margin-left: 3px;
}
.spec-scroll .items ul li img{border:1px solid #CCC;padding:1px;width:50px;height:50px;}
.spec-scroll .items ul li img:hover{border:2px solid #FF6600;padding:1px;}
.spec-scroll li .btn{ padding: 0px 4px; margin-bottom: 4px;}


/*图片滚动*/
.slideBox{ width:1200px; height:230px; overflow:hidden; position:relative;  }
		.slideBox .hd{ height:15px; overflow:hidden; position:absolute; right:5px; bottom:5px; z-index:1; }
		.slideBox .hd ul{ overflow:hidden; zoom:1; float:left;  }
		.slideBox .hd ul li{ float:left; margin-right:2px;  width:15px; height:15px; line-height:14px; text-align:center; background:#fff; cursor:pointer; }
		.slideBox .hd ul li.on{ background:#f00; color:#fff; }
		.slideBox .bd{ position:relative; height:100%; z-index:0;   }
		.slideBox .bd li{ zoom:1; vertical-align:middle; }
		.slideBox .bd img{
		 	width:130px;
			height:130px;
			border: 1px solid #ccc;
			padding: 1px;


		}

		.slideBox .prev,
		.slideBox .next{
			position:absolute;
		 	right:3%;
		 	top:30%;
		 	margin-top:-25px;
		 	display:block;
		 	width:35px; height:35px;
		 	filter:alpha(opacity=80);
		 	opacity:0.8;
		 	border-radius: 100%;
		 	border: 1px solid #0157B9;
		 	 }
		.slideBox .next{
		 	left:auto; right:3%;
		 	top:70%;
		   }
		.slideBox .prev:hover,
		.slideBox .next:hover{ filter:alpha(opacity=100);opacity:1;  }
		.slideBox .prevStop{ filter:alpha(opacity=10);opacity:0.1;  }
		.slideBox .nextStop{filter:alpha(opacity=10);opacity:0.1; }
