$(function () {
		//选择商品
        $(".select").click(function(){
        	location.href="../productItem/discountList?typeId=2&discountId="+$(this).attr("data-coupon-id");
		});
		
    	//启用停用
        $(".pause").click(function(){
        	location.href="../coupon/pause/"+$(this).attr("data-coupon-id");
		});
		
		//发放
        $(".give").click(function(){
        	location.href="../coupon/giveInit/"+$(this).attr("data-coupon-id");
		});
		
		//编辑
        $(".edit").click(function(){
        	location.href="../coupon/editInit/"+$(this).attr("data-coupon-id");
		});
	});